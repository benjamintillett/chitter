<% if @user %>
	<%= "User: #{@user.email}" %>
<% else %>
	<a href="users/new">Sign Up</a>

<form action="/sessions/new" method="post">
	Email: <input type="text" name="user_email">
	Password: <input type="password" name="user_password">
	<input type="submit" value="Sign In">
</form>
<% end %>



<%= yield %>